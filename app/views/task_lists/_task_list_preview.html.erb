<div class='card-body container-lg', id="<%= dom_id task_list %>">
  <h3 class='card-title'>
    <%= task_list.name %>
  </h3>
  <hr/>
  <%if task_list.tasks.empty? %>
    <p class='text-center'>Nenhuma tarefa adicionada</p>
  <%else %>
    <h5 class='card-text text-center'>Ãšltimas tarefas</h5> 
    <div class='list-group gap-2'>
     <% task_list.next_tasks.each do |task| %>
       <%= render partial: "task_table_preview", locals: { task: task } %>
     <% end %>
    </div>
  <%end %>
</div>
